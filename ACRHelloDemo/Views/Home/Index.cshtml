@{
    ViewData["Title"] = "Azure Container Registry :: Geo-replication Demo";
}
<style>

    body {
        background-image: url('images/Regions.JPG');
        background-size: cover;
    }

    .footer {
        position: fixed;
        bottom: 0px;
        width: 100%;
    }
</style>
<h1 style="text-align:center;color:white">Hello from:  @ViewData["REGION"]</h1>
<div class="footer">
    <ul style="color:white">
        <li>Registry URL: @ViewData["REGISTRYURL"]</li>
        <li>Registry IP: @ViewData["REGISTRYIP"]</li>
        <li>Registry Region: @ViewData["REGION"]</li>
    </ul>
</div>
